{% extends 'base.html.twig' %}

{% block title %}Genre{% endblock %}

{% block body %}
<div class="container">
    <h1>Genre</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ genre.id }}</td>
            </tr>
            <tr>
                <th>Nom</th>
                <td>{{ genre.nom }}</td>
            </tr>
        </tbody>
    </table>

    

    <a href="{{ path('genre_edit', {'id': genre.id}) }}" class="btn btn-success mt-1 mb-1">Modifier</a>

   </div>
{% endblock %}
